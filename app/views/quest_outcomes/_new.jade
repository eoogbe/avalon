section(data-bind='if: $root.isCurrentPage("new_quest_outcome")')
  h2 Choose your desired quest outcome
  
  button.quest-outcome-btn(
    type='button'
    data-bind='click: quest().createSuccessOutcome'
  )
    img.img-responsive(
      src='/images/success.jpg'
      alt='Success'
      width='187'
      height='289'
      aria-hidden='true'
    )
    span.sr-only Success
  button.quest-outcome-btn(
    type='button'
    data-bind='click: quest().createFailOutcome, disable: player().isGood'
  )
    img.img-responsive(
      alt='Fail'
      width='187'
      height='289'
      aria-hidden='true'
      data-bind='attr: { src: $root.quest().failOutcomeImg() }'
    )
    span.sr-only Fail
